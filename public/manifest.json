{
  "manifest_version": 3,
  "name": "Center",
  "version": "0.1",
  "description": "For staying in touch with my famiy.",
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": [
    "storage"
  ],
  "author": "Joseph Tey",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "short_name": "Center",
  "action": {
    "default_title": "Center",
    "default_icon": {
      "16": "/images/tree-icon-16.png",
      "32": "/images/tree-icon-32.png",
      "48": "/images/tree-icon-48.png",
      "128": "/images/tree-icon-128.png"
    }
  },
  "icons": {
    "16": "/images/tree-icon-16.png",
    "32": "/images/tree-icon-32.png",
    "48": "/images/tree-icon-48.png",
    "128": "/images/tree-icon-128.png"
  }
}